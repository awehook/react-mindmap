{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["off", "always"],
        "quotes": ["off", "double"],
        "brace-style": ["off", "stroustrup"],
        "eqeqeq": ["off", "smart"],
        "indent": ["off", "double"],
        "no-trailing-spaces": ["off"]
    }
}
